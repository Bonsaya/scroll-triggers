{
  "name": "scroll-triggers",
  "description": "A tiny, dependency-less javascript library to add classes as elements scroll into viewport.",
  "homepage": "https://github.com/firstandthird/scroll-triggers",
  "author": "First+Third",
  "license": "MIT",
  "version": "1.0.0",
  "main": "dist/scroll-triggers.js",
  "scripts": {
    "build": "babel index.js --out-file dist/scroll-triggers.js --source-maps",
    "dev": "concurrently -r 'nswatch' 'static-server -p 8000'",
    "lint": "eslint index.js",
    "prepublish": "npm run build"
  },
  "watch": {
    "index.js": ["lint", "build"]
  },
  "keywords": [
    "browser",
    "scroll",
    "declaritive"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/firstandthird/scroll-triggers.git"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^2.2.0",
    "eslint": "^3.5.0",
    "eslint-config-firstandthird": "^3.0.2",
    "eslint-plugin-import": "^1.15.0",
    "nswatch": "^0.2.0",
    "static-server": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "firstandthird"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
